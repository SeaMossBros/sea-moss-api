{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "total": {
      "type": "decimal",
      "required": true,
      "min": 0
    },
    "user_email": {
      "type": "email",
      "required": false
    },
    "cart": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::cart.cart"
    },
    "payment_session_id": {
      "type": "uid",
      "required": true,
      "private": true
    },
    "payment_status": {
      "type": "enumeration",
      "enum": [
        "pending",
        "success",
        "cancelled"
      ],
      "default": "pending",
      "required": true
    },
    "shipping_address": {
      "type": "string"
    },
    "tracking_url_provider": {
      "type": "string"
    },
    "label_url": {
      "type": "string"
    },
    "customer_experience": {
      "type": "string"
    }
  }
}
